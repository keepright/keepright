msgid "Data Checks for Europe%s (Starting point Vienna, Austria)"
msgstr ""
"Comprobaciones de datos para Europa%s (punto de inicio Viena, Austria)"

#: index.php:27
msgid "Australian KeepRight partner site"
msgstr "Sitio socio australiano de KeepRight"

#: index.php:30
#, php-format
msgid "Data Checks for Africa%s (Starting point Cairo, Egypt)"
msgstr ""
"Comprobaciones de datos para África%s (punto de inicio El Cairo, Egipto)"

#: index.php:33
#, php-format
msgid "Data Checks for Canada%s (Starting point Québec)"
msgstr "Comprobaciones de datos para Canadá%s (punto de inicio Québec)"

